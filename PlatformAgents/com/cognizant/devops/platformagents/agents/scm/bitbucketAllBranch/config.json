{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg":"iAgent"
  },
  "subscribe": {
    "config": "SCM.BITBUCKET.config"
  },
  "publish": {
    "data": "SCM.BITBUCKET.DATA",
    "health": "SCM.BITBUCKET.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": true,
    "responseType": "JSON"
  },
    "dynamicTemplate": {
	  "responseTemplate": {
	    "id": "commitId",
	    "author": {
	      "name": "authorName",
	      "emailAddress": "authorEmail"
	    },
	    "authorTimestamp": "createdTime",
	    "message": "commitMessage",
	    "properties": {
	      "jira-key": [
	        "jiraKey"
	      ]
	    },
	    "pullRequests": {
	      "id" : "pullReqId",
	      "state" : "pullReqState",
	      "createdDate" : "createdTime",
	      "updatedDate" : "updatedTime",
	      "closedDate" : "closedTime",
	      "fromRef" : {
	        "displayId" : "fromBranch"
	      },
	      "toRef" : {
	        "displayId" : "toBranch"
	      },
	      "author": {
	        "user": {
	          "name": "authorName",
	          "emailAddress": "authorEmail"
	        }
	      }
	    }
	  }
  },
  "toolCategory" : "SCM",
  "runSchedule": 30,
  "schedulerStatus": "None",
  "toolsTimeZone" : "GMT",
  "insightsTimeZone" : "GMT",
  "enableValueArray": false,
  "dataBatchSize": "None",
  "startFrom" : "2017-07-01 00:00:00",
  "useResponseTemplate" : true,
  "scanAllBranches" : false,
  "scanPullRequests": true,
  "auth": "base64",
  "userID": "",
  "passwd": "",
  "timeStampField":"createdTime",
  "timeStampFormat":"%Y-%m-%dT%H:%M:%S",
  "timeFieldMapping": "None",
  "isEpochTimeFormat" : true,
  "baseEndPoint": "http://127.0.0.1:7990/rest/api/1.0/projects/",
  "isDebugAllowed" : false,
  "loggingSetting" : {
        "logLevel" : "WARN",
		"maxBytes" : 5000000,
		"backupCount" : 1000
    }
}
