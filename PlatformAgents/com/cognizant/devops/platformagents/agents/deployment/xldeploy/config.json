{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "localhost",
    "exchange": "iSight",
    "agentControlXchg":"iAgent"
  },
  "subscribe": {
    "config": "DEPLOYMENT.XLDEPLOY.config"
  },
  "publish": {
    "data": "DEPLOYMENT.XLDEPLOY.DATA",
    "health": "DEPLOYMENT.XLDEPLOY.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": true,
    "responseType": "JSON"
  },
 "dynamicTemplate": {
	"responseTemplate" : {
		"sha": "git_commitId",
		"commit" : {
			"author" : {
				"name": "git_authorName",
				"date": "git_commiTime"
			}
		}
	},
	"clientConfig": {
	    "runSchedule": {
	      "userInput": true,
	      "label": "Schedule interval in minutes",
	      "type": "input",
	      "value": 30,
	      "errorMessage": "Please specify schedule interval",
	      "required": true
	    }
     }	
  },
 "auth": "base64",
