 "timeStampFormat":"%Y-%m-%dT%H:%M:%S",
 "timeFieldMapping": "None",
 "timeStampField":"startDate",
 "useResponseTemplate" : true,
 "dataBatchSize": "None",
 "userID": "userdi",
 "passwd": "passwd",
  "baseEndPoint": "http://127.0.0.1:4516/deployit",
  "isDebugAllowed" : false,
  "loggingSetting" : {
		"logLevel" : "WARN",
		"maxBytes" : 5000000,
